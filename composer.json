{
    "name": "boxed-code/laravel-twofactor",
    "description": "Middleware based, multi-provider two factor authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "green2go@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/framework": "^5.6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BoxedCode\\Laravel\\TwoFactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoxedCode\\Laravel\\TwoFactor\\TwoFactorServiceProvider"
            ]
        }
    },
    "suggest": {
        "pragmarx/google2fa": "Allows use of Google Authenticator (OTP)",
        "bacon/bacon-qr-code": "Allows local generation of QR code images",
        "laravel-notification-channels/twilio": "Allows the use of twilio SMS & voice"
    }
}
